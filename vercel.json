{
  "version": 2,
  "functions": {
    "api/*.php": {
      "runtime": "@vercel/php"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|ico|svg|ttf|woff|woff2)$)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      },
      "dest": "/public/$1"
    },
    {
      "src": "/",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "PHP_VERSION": "8.2"
  },
  "build": {
    "env": {
      "PHP_VERSION": "8.2"
    }
  }
}